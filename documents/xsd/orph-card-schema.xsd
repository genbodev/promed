                      <xs:restriction base="xs:int">
                        <xs:enumeration value="1" />
                        <xs:enumeration value="2" />
                        <xs:enumeration value="3" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="lechen" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>
