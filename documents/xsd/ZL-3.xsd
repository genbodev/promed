						<xs:element name="VPOLIS" type="xs:int" minOccurs="1" maxOccurs="1"/>
						<xs:element name="SPOLIS" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="NPOLIS" type="xs:string" minOccurs="1" maxOccurs="1"/>
						<xs:element name="DATE_BEGIN" type="xs:date" minOccurs="0" maxOccurs="1"/>
						<xs:element name="SMO" type="xs:int" minOccurs="1" maxOccurs="1"/>
						<xs:element name="STREET_REG" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="HOUSE_REG" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="CORPUS_REG" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="FLAT_REG" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="STREET_PROG" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="HOUSE_PROG" type="xs:string" minOccurs="0" maxOccurs="1"/>
