						<xs:element name="FLAT_PROG" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="CASE_VID" type="xs:int"  minOccurs="1" maxOccurs="1"/>
						<xs:element name="CODE_MO" type="xs:int"  minOccurs="1" maxOccurs="1"/>
						<xs:element name="DATE_END" type="xs:date"  minOccurs="0" maxOccurs="1"/>
						<xs:element name="FAM_OLD" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="IM_OLD" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="OT_OLD" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="DR_OLD" type="xs:date" minOccurs="0" maxOccurs="1"/>
						
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>	
	</xs:complexType>
</xs:element>
</xs:schema>
