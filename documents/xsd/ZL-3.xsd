<?xml version="1.0" encoding="Windows-1251"?>
<xs:schema xmlns:tns="http://uir.ffoms.ru" xmlns:xs="http://www.w3.org/2001/XMLSchema" target-Namespace="http://uir.ffoms.ru" elementFormDefault="qualified">
<xs:element name="PERS_LIST">
	<xs:complexType>
		<xs:sequence>
			<xs:element name="ZGLV" minOccurs="1" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="VERSION" type="xs:string" minOccurs="1" maxOccurs="1"/>
						<xs:element name="DATA" type="xs:date" minOccurs="1" maxOccurs="1"/>
						<xs:element name="FILENAME" type="xs:string" minOccurs="1" maxOccurs="1" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>	
			<xs:element name="PERS" minOccurs="1" maxOccurs="unbounded" >
				<xs:complexType>
					<xs:sequence>
						<xs:element name="FAM" type="xs:string" minOccurs="1" maxOccurs="1"/>
						<xs:element name="IM" type="xs:string" minOccurs="1" maxOccurs="1"/>
						<xs:element name="OT" type="xs:string" minOccurs="0" maxOccurs="1"/>
